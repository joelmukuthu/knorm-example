  },
  "homepage": "https://github.com/joelmukuthu/knorm-example#readme",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-ganintegrity": "^2.0.2",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "knex": "^0.14.4",
    "knorm": "^0.11.0",
    "knorm-timestamps": "^3.0.2",
    "lodash": "^4.17.5",
    "pg": "^7.4.1"
